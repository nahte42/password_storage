#include<iostream>
#include<vector>
#include<fstream>
#include<cstdlib>
#include<time.h>

using namespace std;

int main(){
  srand(time(0));
  string name = "passwordfile.txt";
  fstream password;
  password.open(name.c_str(),ios::in);

  string fullist;

  password >> fullist;

  for(int i = 0; i < fullist.length(); i++){
    fullist[i] += rand()%128;
    fullist[i] %= 128;
  }
  cout<< fullist<<endl;
  return 0;

}
